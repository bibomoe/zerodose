{
    "openapi": "3.0.0",
    "info": {
      "title": "Immunization API",
      "description": "API for managing immunization data and user login.",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "http://localhost/zerodozed/api",
        "description": "Local API Server"
      }
    ],
    "paths": {
      "/users/login": {
        "post": {
          "summary": "User login and generate a token",
          "operationId": "loginUser",
          "tags": ["Users"],
          "requestBody": {
            "description": "The user credentials for login",
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "username": {
                      "type": "string",
                      "example": "asik"
                    },
                    "password": {
                      "type": "string",
                      "example": "password123"
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Login successful, token generated.",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "boolean",
                        "example": true
                      },
                      "message": {
                        "type": "string",
                        "example": "Login successful"
                      },
                      "token": {
                        "type": "string",
                        "example": "d80be8286b5993272b9084c0a8aec06e"
                      }
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Missing username or password"
            },
            "401": {
              "description": "Invalid username or password"
            }
          }
        }
      },
      "/asik": {
        "post": {
          "summary": "Add or update immunization data",
          "operationId": "addImmunizationData",
          "tags": ["Immunization"],
          "requestBody": {
            "description": "The immunization data to be inserted or updated.",
            "required": true,
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "province": {
                      "type": "string",
                      "example": "99 - TES"
                    },
                    "city": {
                      "type": "string",
                      "example": "9901 - KOTA TES JUGA"
                    },
                    "subdistrict": {
                      "type": "string",
                      "example": "110201 - LAWE ALAS"
                    },
                    "puskesmas": {
                      "type": "string",
                      "example": "1032292 - PANDAK I"
                    },
                    "year": {
                      "type": "integer",
                      "example": 2025
                    },
                    "month": {
                      "type": "integer",
                      "example": 3
                    },
                    "dpt_hb_hib_1": {
                      "type": "integer",
                      "example": 100
                    },
                    "dpt_hb_hib_2": {
                      "type": "integer",
                      "example": 120
                    },
                    "dpt_hb_hib_3": {
                      "type": "integer",
                      "example": 130
                    },
                    "mr_1": {
                      "type": "integer",
                      "example": 141
                    }
                  }
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Immunization data added successfully",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "boolean",
                        "example": true
                      },
                      "message": {
                        "type": "string",
                        "example": "Immunization data added successfully"
                      }
                    }
                  }
                }
              }
            },
            "200": {
              "description": "Immunization data updated successfully",
              "content": {
                "application/json": {
                  "schema": {
                    "type": "object",
                    "properties": {
                      "status": {
                        "type": "boolean",
                        "example": true
                      },
                      "message": {
                        "type": "string",
                        "example": "Immunization data updated successfully"
                      }
                    }
                  }
                }
              }
            },
            "400": {
              "description": "Invalid data provided"
            },
            "401": {
              "description": "Invalid or missing token"
            },
            "500": {
              "description": "Internal server error"
            }
          }
        }
      }
    }
  }
  